# Production Environment Variables for Docker

# Application  
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# ML Model Service (localhost since all services are on same server)
NEXT_PUBLIC_ML_MODEL_URL=http://localhost:13000

# MQTT WebSocket (localhost since all services are on same server)
NEXT_PUBLIC_MQTT_WS_URL=ws://localhost:9001

# Video Configuration
NEXT_PUBLIC_VIDEO_BASE_PATH=/videos
NEXT_PUBLIC_DEFAULT_VIDEO=video01.mp4

# API Base URL
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000 